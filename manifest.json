{
    "name": "GetXPath",
    "description": "Free Extension - Xpath finder",
    "version": "1.0",
    "manifest_version": 2,
    "icons": {
        "128": "assets/icon_16.png",
        "128": "assets/icon_32.png",
        "128": "assets/icon_48.png",
        "128": "assets/icon_128.png"
    },
    "permissions": [
        "<all_urls>"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "app/src/content.js"
            ]
        }
    ]
}